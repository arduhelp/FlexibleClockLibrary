      u8g2.drawStr(30, 60, WiFiMenuItems[parampamw + 1]);
      if(digitalRead(okpin) == oksig){}
      //u8g2.sendBuffer();
      break;

    case 5: // exit
      u8g2.drawXBMP(0, 25, 120, 24, item_bitmap_frame);  
      u8g2.drawXBMP(5, 28, 15, 15, item_bitmap_exit_enter);
      u8g2.drawStr(30, 22, WiFiMenuItems[parampamw - 1]); 
      u8g2.drawStr(30, 40, WiFiMenuItems[parampamw]);
      u8g2.drawStr(30, 60, WiFiMenuItems[0]);
      if(digitalRead(okpin) == oksig){delay(1000); return;}
      //u8g2.sendBuffer();
      break;

    default:
      parampamw = 0; // Повертаємось до першого пункту, якщо вийшли за межі
      break;
  }
  u8g2.drawStr(126, parampamw * 8, ".");

  // Автоматичний фліп
  if (clockLib.autofliper == 1) {
    if (millis() - lastTime >= 1000) { // Якщо пройшла 1 секунда
      lastTime = millis(); // Оновлюємо таймер
      u8g2.clearBuffer();
      parampamw++; // Переходимо до наступного пункту
      u8g2.sendBuffer();
      if (parampamw > 5) { // Якщо перевищили кількість пунктів меню
        parampamw = 0; // Повертаємось до першого пункту
      }
    }
  }

  u8g2.sendBuffer(); // Оновлюємо екран
  delay(10);
  }
  }

